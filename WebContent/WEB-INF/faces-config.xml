<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>administrador</managed-bean-name>
		<managed-bean-class>com.Proyecto.Modelo.Administrador</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sesion</managed-bean-name>
		<managed-bean-class>com.Proyecto.Sesiones.SessionManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ruta</managed-bean-name>
		<managed-bean-class>com.Proyecto.Modelo.Ruta</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>daoRuta</managed-bean-name>
		<managed-bean-class>com.Proyecto.AccesoaDatos.DAORuta</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>marca</managed-bean-name>
		<managed-bean-class>com.Proyecto.Modelo.Marca</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dAOMarca</managed-bean-name>
		<managed-bean-class>com.Proyecto.AccesoaDatos.DAOMarca</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>daoadmin</managed-bean-name>
		<managed-bean-class>com.Proyecto.AccesoaDatos.DAOAdministrador</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>daoDestino</managed-bean-name>
		<managed-bean-class>com.Proyecto.AccesoaDatos.DAODestino</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>destino</managed-bean-name>
		<managed-bean-class>com.Proyecto.Modelo.Destino</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>posicion</managed-bean-name>
		<managed-bean-class>com.Proyecto.Modelo.Posicion</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>daoPosiciones</managed-bean-name>
		<managed-bean-class>com.Proyecto.AccesoaDatos.DAOPosiciones</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-action>users</from-action>
			<from-outcome>users</from-outcome>
			<to-view-id>/bienvenidaUsuarios.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-action>admin</from-action>
			<from-outcome>admin</from-outcome>
			<to-view-id>/bienvenidaAdmin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>bienvenidaAdmin</display-name>
		<from-view-id>/bienvenidaAdmin.jsp</from-view-id>
		<navigation-case>
			<from-action>mapa</from-action>
			<from-outcome>mapa</from-outcome>
			<to-view-id>/mapa.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
